@{ Layout = "_Layout"; }

<h1>UnSugar Coded</h1>
<h2>Create User Account</h2>
<hr>

<form action="/logins" method="post">
  <label for="loginName">User name:</label>
  <input type="text" name="loginName" id="loginName">
  <label for="loginEmail">Email address:</label>
  <input type="text" name="loginEmail" id="loginEmail">
  <label for="loginHeight">Height in inches:</label>
  <input type="text" name="loginHeight" id="loginHeight">
  <label for="loginWeight">Weight in pounds:</label>
  <input type="text" name="loginWeight" id="loginWeight">
  <button type="submit">Create Profile</button>
</form>

<h4><a href="/"></a>Return to home</h4>
