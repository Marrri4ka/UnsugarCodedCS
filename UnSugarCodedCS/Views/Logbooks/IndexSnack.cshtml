<!DOCTYPE html>

<html>

<body>
  @for (int i = 0; i < ViewBag.Snack.Count; i++)
  {
  @ViewBag.Snack[@i].GetSnackFood();
  <br />
  @ViewBag.Snack[@i].GetSnackSugar();
  <br />
  @ViewBag.Snack[@i].GetSnackStampTime();
  <br />
  @ViewBag.Snack[@i].GetSnackCarb();
  <br />
  <form action="/snack/deletesnack" method="post">
      <button class="btn btn-sm" type="submit" name="button">Delete Snack</button>
  </form>
  }
<h4><a href="/logins/@Model.GetId()/logbooks/new">Return to logbook</a></h4>
<h4><a href="/">Return to Home</a></h4>
</body>

</html>
